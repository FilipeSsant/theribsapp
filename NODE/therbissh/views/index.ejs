<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
        <style media="screen">
            *{
                padding: 0;
                margin: 0;
            }
            body{
                background-color: #313131;
                width: 100%;
                height: auto;
            }
            #tabela{
                width:800px;
                height: auto;
                margin-top: 50px;
                margin-left: auto;
                margin-right: auto;
                background-color: #ddd;
                border-collapse: collapse;

            }
            #tabela tr td{
                text-align: center;
                border: solid 1px #000;
            }
            #tabela tr th{
                font-weight: bold;
            }
        </style>
    </head>
    <body>
        <table id="tabela">
            <tr>
                <th>Data</th>
                <th>Titulo</th>
                <th>Descricao</th>
            </tr>
            <% for(var i=0; i < app_name.length; i++) {
                %>
                <tr>
                   <td><%= app_name[i].data %></td>
                   <td><%= app_name[i].titulo %></td>
                   <td><%= app_name[i].descricao %></td>
                </tr>
            <% } %>
        </table>
    </body>
</html>
